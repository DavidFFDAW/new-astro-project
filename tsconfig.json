{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@database/*": [
        "src/database/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@css/*": [
        "src/css/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@dao/*": [
        "src/database/models/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}